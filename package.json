{
    "name": "dharma-syntax",
    "displayName": "Dharma Syntax",
    "description": "Visual Studio Code extension providing syntax highlighting for Dharma files.",
    "version": "0.3.0",
    "publisher": "Qavar",
    "engines": {
        "vscode": "^1.8.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "dharma",
        "dg"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dharma",
                "aliases": [
                    "Dharma",
                    "dharma"
                ],
                "extensions": [
                    ".dg"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dharma",
                "scopeName": "source.dg",
                "path": "./syntaxes/dharma.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/qavar/vscode-dharma-syntax.git"
    },
    "homepage": "https://github.com/qavar/vscode-dharma-syntax"
}